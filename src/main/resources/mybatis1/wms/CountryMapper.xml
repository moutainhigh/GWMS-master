<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CountryMapper">
	
	<!--表名 -->
	<sql id="tableName">
		WMS_COUNTRY
	</sql>
	
	<!-- 字段 -->
	<sql id="Field">
		COUN_CODE,	
		COUN_C_NAME,	
		COUN_E_NAME,	
		REMARK,	
		COUNTRY_ID
	</sql>
	
	<!-- 字段值 -->
	<sql id="FieldValue">
		#{COUN_CODE},	
		#{COUN_C_NAME},	
		#{COUN_E_NAME},	
		#{REMARK},	
		#{COUNTRY_ID}
	</sql>
	
	<!-- 新增-->
	<insert id="save" parameterType="pd">
		insert into 
	<include refid="tableName"></include>
		(
	<include refid="Field"></include>
		) values (
	<include refid="FieldValue"></include>
		)
	</insert>
	
	<!-- 删除-->
	<delete id="delete" parameterType="pd">
		delete from
		<include refid="tableName"></include>
		where 
			COUNTRY_ID = #{COUNTRY_ID}
	</delete>
	
	<!-- 修改 -->
	<update id="edit" parameterType="pd">
		update
		<include refid="tableName"></include>
		set 
			COUN_CODE = #{COUN_CODE},
			COUN_C_NAME = #{COUN_C_NAME},
			COUN_E_NAME = #{COUN_E_NAME},
			REMARK = #{REMARK},
		COUNTRY_ID = COUNTRY_ID
		where 
		COUNTRY_ID = #{COUNTRY_ID}
	</update>
	
	<!-- 通过ID获取数据 -->
	<select id="findById" parameterType="pd" resultType="pd">
		select 
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 
			COUNTRY_ID = #{COUNTRY_ID}
	</select>
	
	<!-- 列表 -->
	<select id="datalistPage" parameterType="page" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 1=1
		<if test="pd.COUN_CODE!= null and pd.COUN_CODE != ''"><!-- 关键词检索 -->
			and COUN_CODE LIKE "%"#{pd.COUN_CODE}"%"
		</if>
        <if test="pd.COUN_C_NAME!= null and pd.COUN_C_NAME != ''"><!-- 关键词检索 -->
            and COUN_CCOUN_C_NAMEODE LIKE "%"#{pd.COUN_C_NAME}"%"
        </if>
        <if test="pd.COUN_E_NAME!= null and pd.COUN_E_NAME != ''"><!-- 关键词检索 -->
            and COUN_E_NAME LIKE "%"#{pd.COUN_E_NAME}"%"
        </if>
	</select>
	
	<!-- 列表(全部) -->
	<select id="listAll" parameterType="pd" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
	</select>
	
	<!-- 批量删除 -->
	<delete id="deleteAll" parameterType="String">
		delete from
		<include refid="tableName"></include>
		where 
			COUNTRY_ID in
		<foreach item="item" index="index" collection="array" open="(" separator="," close=")">
                 #{item}
		</foreach>
	</delete>
	
	<!-- gwms9551942qq -->
</mapper>